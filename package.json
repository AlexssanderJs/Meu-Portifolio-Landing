{
  "name": "projeto-final-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "npx jest",
    "test:watchAll": "npx jest --watchAll",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "serve:ssr:projeto_final_angular": "node dist/projeto_final_angular/server/server.mjs"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.1",
    "@angular/compiler": "^20.3.1",
    "@angular/core": "^20.3.1",
    "@angular/forms": "^20.3.1",
    "@angular/platform-browser": "^20.3.1",
    "@angular/platform-server": "^20.3.1",
    "@angular/router": "^20.3.1",
    "@angular/ssr": "^20.3.2",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.2",
    "@angular/cli": "^20.3.2",
    "@angular/compiler-cli": "^20.3.1",
    "@angular/language-service": "^20.2.4",
    "@types/express": "^5.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.17.19",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^15.0.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  }
}
